{
  "manifest_version": 3,
  "name": "AIMS Helper",
  "version": "2.0",
  "description": "Autofills CAPTCHA on the login page and exports course timetables to your calendar.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://aims.iith.ac.in/aims/",
        "https://aims.iith.ac.in/aims/login/loginHome"
      ],
      "js": ["captcha.js"]
    }
  ]
}